  <code><![CDATA[globals [ 
 ; influence-type
 ; percent-option-B
 ; agents-updated-per-tick 
 ; spatial-interactions?
 pref-A
 pref-B
]
; turtles-own [opinion]

patches-own [
  opinion
]

;;Crear una tortuga por patch y dar opinión inicial aleatoria
to setup
  clear-all
;  ask patches[
;    sprout 1[
;     ; set opinion (random-float 2) - 1 ;;opinión inicial uniforme entre [-1, 1]
;      set shape "circle"
;    ]
;  ]
  initial-opinions
  update-colors ;;colorear de acuerdo a opinión
  update-preferences ;; Contar agentes con preferencia por A y por B
  reset-ticks
end

to initial-opinions
;  ask turtles [
;    set opinion (random-normal 0.5 0.1) ; Primero a todas se les da preferencia por la opción A
;  ]
;  ask n-of((percent-option-B / 100) * (count turtles) ) turtles [; Se inicia un porcentaje con preferencia con B. 39% en base a las encuestas 
;  set opinion (random-normal -0.5 0.1) 
;  ]
  ask patches[
    set opinion random-float 1
  ]
  ask n-of ( (percent-option-B / 100) * (count patches) ) patches [
    set opinion random-float -1
  ]
end

to go
  ;; dos agentes se eligen de manera aleatoria para interactuar
  ask n-of agents-updated-per-tick patches [
    let x1 opinion 
    let other-patch one-of other patches 
    
    if spatial-interactions? 
      [set other-patch one-of neighbors4] 
    
    let x2 [opinion] of other-patch
    ;; Influencia positiva. Todos se hacen más iguales
    if influence-type = "positive"
    [let x1-new (x1 + learning-rate * (x2 - x1))
    let x2-new (x2 + learning-rate * (x1 - x2))
    set opinion x1-new
      ask other-patch [ set opinion x2-new]]
    
    ;; Confianza acotada. Solo te afectan los que se parecen a ti
    if influence-type = "BC"
    [if (abs (x1 - x2) < confidence-threshold) [ ; Son suficientemente iguales?
      let x1-new (x1 + learning-rate * (x2 - x1))
      let x2-new (x2 + learning-rate * (x1 - x2))
      set opinion x1-new
      ask other-patch [ set opinion x2-new]
      ]
    ]
    
    ;; Influencia negativa. Opiniones muy opuestas se hacen más extremas
    if influence-type = "negative"
    [ if (abs (x1 - x2) < confidence-threshold) [ ; Son suficientemente iguales?
      let x1-new (x1 + learning-rate * (x2 - x1))
      let x2-new (x2 + learning-rate * (x1 - x2))
      set opinion x1-new
      ask other-patch [ set opinion x2-new]
      ]
      if (abs (x1 - x2) > confidence-threshold)[ ; Son muy diferentes?
        ifelse (x1 > x2)
        [
          let x1-new (x1 + learning-rate * (x1 - x2) * (1 - x1) * .5)
          let x2-new (x2 + learning-rate * (x2 - x1) * (1 + x2) * .5)
          set opinion x1-new
          ask other-patch [ set opinion x2-new]
        ]
        [
          let x1-new (x1 + learning-rate * (x1 - x2) * (1 + x1) * .5)
          let x2-new (x2 + learning-rate * (x2 - x1) * (1 - x2) * .5)
          set opinion x1-new
          ask other-patch [ set opinion x2-new]
        ]
      ]
    ]     
  ]
  update-colors
  update-preferences
  tick
  if ticks = 260 [
    update-colors
    update-preferences
    stop
  ] 
end

;; Color de agentes en base a la opinión
to update-colors
  ask patches [ if-else opinion > 0
    [ set pcolor scale-color red opinion 0 1]
    [ set pcolor scale-color blue opinion -1 0]
  ]
end

;; Se agrega contador de agentes con preferencias por A y B, para reporte en los experimentos
to update-preferences
  set pref-A (count patches with [opinion > 0] )
  set pref-B (count patches with [opinion < 0] )
end]]></code>
